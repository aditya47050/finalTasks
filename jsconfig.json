{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*"],
"exclude": [
  "node_modules",
  "**/node_modules",
  "build",
  ".next",
  "public",
  "prisma",
  ".git",
  ".vscode",
  "coverage",
  "dist"
]
}